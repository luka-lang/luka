import "stdio";

struct Person {
    name: mut u8*,
    age: mut u8
}

fn main(): s32 {
    let mut bob = Person { name: "Bob", age: 14 as u8 };

    printf("Person\n\tName: %s\n\tAge: %d\n", bob.name, bob.age);

    bob.age = bob.age + 1;

    printf("Person\n\tName: %s\n\tAge: %d\n", bob.name, bob.age);
}
