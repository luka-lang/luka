extern puts(str: u8*): s32;
extern printf(format: u8*, ...: any): s32;
extern scanf(format: u8*, ...: mut any): s32;
extern putchar(ch: int): s32;

let EOF = -1;

fn print(str: u8*)
{
    printf("%s", str)
}

fn println(str: u8*)
{
    puts(str)
}

fn scan_s32(var: s32 mut*)
{
    scanf("%d", var)
}
