import "lib/stdio";

fn fib(n: u64): u64 {
    if n <= 0 {
        0 as u64
    } else {
       if n == 1 {
           1 as u64
       } else {
           fib(n - 1) + fib(n - 2)
       }
    }
}

fn main() {
    printf("Give me an index and I will print the Fibonacci series up to that index: ");
    let index: u64 = 0;
    scanf("%zu", &index);

    let mut i = 0;
    while i < index {
        printf("Fibonacci(%d): %d\n", i, fib(i));
        i = i + 1;
    }
}
